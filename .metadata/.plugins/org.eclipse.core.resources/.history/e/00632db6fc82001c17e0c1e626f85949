package com.healthasyst.practise;

public class MultiplicationTableOf5 {
	
	public static int Sample4(int a, int b){
		
		int c;
		int sumofeven=0;
		int sumofodd;
		
		for (b=1;b<=30;b++) 
		{
			c=a*b;
			System.out.println(+a+"*"+b+ "=" + c);
			if(c%2==0)
			{
				sumofeven=sumofeven+c;
				System.out.println(+c);
				
			}
			
		}	
		
	}

}
