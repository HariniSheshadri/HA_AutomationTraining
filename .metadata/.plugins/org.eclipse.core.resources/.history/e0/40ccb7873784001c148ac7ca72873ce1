package com.HA.volume;

public class Volume {
	
	public void voulmeOfSphere() {
		
	}
	
	public void volumeOfCuboid(double r){
		
		public double V=0;
		
		if(r>0) {
			double temp=4/3;
			V=temp*r*r*r*3.14;
		}
		else {
			System.out.println("Invalid input");
		}
			
		System.out.println("Volume of sphere is" +V);
		
		
	}

}
