package com.healthasyst.practise;

public class BinaryTriangle {
	
	public static void DisplayBinaryTriangle(int rows) {
		
		int x;
		int y;
		for(x=1;x<=rows;x++) {
			
			for(j=1;j<=i;j++) {
				if(i%2==1) {
					System.out.println("0");
				}
				else {
					System.out.println("1");
				}
					
			}
		}
		
	}

}
